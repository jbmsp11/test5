pipeline {
    agent any
    stages {
        stage("Clean Up") {
            steps {
                deleteDir()
            }
        }
        stage("Clone Repo"){
            steps {
                sh "git clone https://github.com/jbmsp11/test5.git "
            }
        }
        stage("Test"){
            steps{
                dir("test3") {
                    sh "python main.py"
            }
        }
    }
}
}